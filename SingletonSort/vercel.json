{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/SingletonSort/browser",
  "framework": null,
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|webp|mp4|webm|ogg|mp3|wav|flac|aac))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.*",
      "dest": "/api/index"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
